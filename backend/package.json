{
  "name": "cine-vision-backend",
  "version": "1.0.0",
  "description": "Cine Vision API - Netflix-like streaming platform with Telegram integration",
  "author": "Cine Vision Team",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migration:generate": "typeorm-ts-node-commonjs migration:generate -d src/config/database.config.ts",
    "migration:create": "typeorm-ts-node-commonjs migration:create",
    "migration:run": "typeorm-ts-node-commonjs migration:run -d src/config/database.config.ts",
    "migration:revert": "typeorm-ts-node-commonjs migration:revert -d src/config/database.config.ts",
    "migration:show": "typeorm-ts-node-commonjs migration:show -d src/config/database.config.ts",
    "schema:sync": "typeorm-ts-node-commonjs schema:sync -d src/config/database.config.ts",
    "seed:run": "ts-node src/database/seeds/run-seeds.ts",
    "upload:movies": "ts-node scripts/upload-initial-movies.ts"
  },
  "dependencies": {
    "@aws-sdk/client-cloudfront": "^3.896.0",
    "@aws-sdk/client-s3": "^3.896.0",
    "@aws-sdk/client-sts": "^3.908.0",
    "@aws-sdk/cloudfront-signer": "^3.893.0",
    "@aws-sdk/lib-storage": "^3.907.0",
    "@aws-sdk/s3-request-presigner": "^3.896.0",
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@google-cloud/storage": "^7.17.2",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/platform-socket.io": "^10.4.20",
    "@nestjs/schedule": "^6.0.1",
    "@nestjs/swagger": "^7.1.17",
    "@nestjs/throttler": "^5.0.1",
    "@nestjs/typeorm": "^10.0.0",
    "@nestjs/websockets": "^10.4.20",
    "@supabase/supabase-js": "^2.39.0",
    "@types/bull": "^3.15.9",
    "@types/fluent-ffmpeg": "^2.1.27",
    "@types/ioredis": "^4.28.10",
    "@types/multer": "^2.0.0",
    "@types/qrcode": "^1.5.5",
    "aws-cloudfront-sign": "^3.0.2",
    "axios": "^1.12.2",
    "bcrypt": "^5.1.1",
    "bull": "^4.16.5",
    "bullmq": "^5.58.8",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "date-fns": "^4.1.0",
    "fluent-ffmpeg": "^2.1.3",
    "googleapis": "^162.0.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.8.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.11.3",
    "qrcode": "^1.5.4",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "socket.io": "^4.8.1",
    "stripe": "^18.5.0",
    "swagger-ui-express": "^5.0.0",
    "typeorm": "^0.3.17",
    "uuid": "^9.0.1",
    "winston": "^3.18.3",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.19.19",
    "@types/passport-jwt": "^3.0.13",
    "@types/passport-local": "^1.0.38",
    "@types/stripe": "^8.0.416",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
