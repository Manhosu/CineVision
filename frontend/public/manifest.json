  "related_applications": [
    {
      "platform": "web",
      "url": "https://cinevision.com/"
    },
    {
      "platform": "telegram",
      "url": "https://t.me/cinevision_bot",
      "id": "cinevision_bot"
    }
  ],

  "prefer_related_applications": false,

  "file_handlers": [
    {
      "action": "/watch",
      "accept": {
        "video/mp4": [".mp4"],
        "video/webm": [".webm"],
        "video/ogg": [".ogv"]
      }
    }
  ],

  "protocol_handlers": [
    {
      "protocol": "web+cinevision",
      "url": "/watch?movie=%s"
    }
  ],

  "share_target": {
    "action": "/share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "movie",
          "accept": ["video/mp4", "video/webm"]
        }
      ]
    }
  },

  "display_override": ["window-controls-overlay", "standalone", "minimal-ui"],

  "edge_side_panel": {
    "preferred_width": 400
  },

  "launch_handler": {
    "client_mode": "focus-existing"
  },

  "id": "cinevision-pwa",
  "dir": "ltr",
  "iarc_rating_id": "e84b072d-71b3-4d3e-86ae-31a8ce4e53b7"
}