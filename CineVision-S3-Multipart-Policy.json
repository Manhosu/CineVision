{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowS3MultipartAndObjects",
      "Effect": "Allow",
      "Action": [
        "s3:CreateMultipartUpload",
        "s3:ListMultipartUploadParts",
        "s3:AbortMultipartUpload",
        "s3:UploadPart",
        "s3:CompleteMultipartUpload",
        "s3:PutObject",
        "s3:GetObject",
        "s3:DeleteObject",
        "s3:ListBucket"
      ],
      "Resource": [
        "arn:aws:s3:::cinevision-videos-prod",
        "arn:aws:s3:::cinevision-videos-prod/*"
      ]
    }
  ]
}